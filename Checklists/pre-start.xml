				<property>/controls/electric/engine[1]/generator</property>
				<value>0</value>
			</binding>
		</item-->
		<item>
			<name>Panel lights</name>
			<value>ON if required</value>
		</item>
		<item>
			<name>Nav lights</name>
			<value>ON</value>
			<marker>
				<x-m>-15.5975</x-m>
				<y-m>-0.2298</y-m>
				<z-m>-0.2372</z-m>
				<scale>1.0000</scale>
			</marker>
			<condition>
				<equals>
					<property>/controls/lighting/nav-lights</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/lighting/nav-lights</property>
				<value>1</value>
			</binding>
		</item>
		<!--item>
			<name>Avionics Master</name>
			<value>ON</value>
		</item-->

		<item>
			<name>Fuel Quantity</name>
			<value>CHECK</value>
			<binding>
				<command>nasal</command>
				<script>gui.showWeightDialog()</script>
			</binding>
		</item>
		<item>
			<name>Request Fuel</name>
			<value>AS REQUIRED</value>
		</item>

		<item>
			<name>Passenger signs</name>
			<value>ON</value>
			<marker>
				<x-m>-15.6521</x-m>
				<y-m>0.0992</y-m>
				<z-m>-0.3107</z-m>
				<scale>2.0909</scale>
			</marker>
			<condition>
				<and>
					<not-equals>
						<property>/controls/switches/no-smoking-sign</property>
						<value>1</value>
					</not-equals>
					<not-equals>
						<property>/controls/switches/seatbelt-sign</property>
						<value>1</value>
					</not-equals>
				</and>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/switches/no-smoking-sign</property>
				<value>2</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>/controls/switches/seatbelt-sign</property>
				<value>0</value>
			</binding>
		</item>
		<item>
			<name>ATIS</name>
			<value>CHECK</value>
			<binding>
				<command>dialog-show</command>
				<dialog-name>weather</dialog-name>
			</binding>
		</item>
		<item>
			<name>Altimeter</name>
			<value>SET</value>
			<marker>
				<x-m>-15.9190</x-m>
				<y-m>-0.7215</y-m>
				<z-m>-1.0521</z-m>
				<scale>1.8760</scale>
			</marker>
		</item>
		<item>
			<name>Clearance</name>
			<value>OBTAIN</value>
		</item>
	</page>
	<page>
		<item>
			<name>Lights</name>
			<value>AS REQUIRED</value>
		</item>
		<item>
			<name>Doors</name>
			<value>CHECK</value>
			<marker>
				<x-m>-15.4785</x-m>
				<y-m>-0.0091</y-m>
				<z-m>-1.2678</z-m>
				<scale>1.0000</scale>
			</marker>		
			<condition>
				<and>
				<not><property>/sim/model/door-positions/aft-cargo/position-norm</property></not>
				<not><property>/sim/model/door-positions/avionic/position-norm</property></not>
				<not><property>/sim/model/door-positions/ctr-cargo/position-norm</property></not>
				<not><property>/sim/model/door-positions/emer-l1/position-norm</property></not>
				<not><property>/sim/model/door-positions/emer-l2/position-norm</property></not>
				<not><property>/sim/model/door-positions/emer-r1/position-norm</property></not>
				<not><property>/sim/model/door-positions/emer-r2/position-norm</property></not>
				<not><property>/sim/model/door-positions/fwd-cargo/position-norm</property></not>
				<not><property>/sim/model/door-positions/pax-left/position-norm</property></not>
				<not><property>/sim/model/door-positions/pax-right/position-norm</property></not>
				</and>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>5</value>
			</binding>		
		</item>
		<item>
			<name>Doors</name>
			<value>CLOSE</value>
			<condition>
				<and>
				<not><property>/sim/model/door-positions/aft-cargo/position-norm</property></not>
				<not><property>/sim/model/door-positions/avionic/position-norm</property></not>
				<not><property>/sim/model/door-positions/ctr-cargo/position-norm</property></not>
				<not><property>/sim/model/door-positions/emer-l1/position-norm</property></not>
				<not><property>/sim/model/door-positions/emer-l2/position-norm</property></not>
				<not><property>/sim/model/door-positions/emer-r1/position-norm</property></not>
				<not><property>/sim/model/door-positions/emer-r2/position-norm</property></not>
				<not><property>/sim/model/door-positions/fwd-cargo/position-norm</property></not>
				<not><property>/sim/model/door-positions/pax-left/position-norm</property></not>
				<not><property>/sim/model/door-positions/pax-right/position-norm</property></not>
				</and>
			</condition>
			<binding>
				<command>nasal</command>
				<script>CRJ700.doors.close();</script>
			</binding>
		</item>
		<item>
			<name>Beacon</name>
			<value>ON</value>
			<marker>
				<x-m>-15.5967</x-m>
				<y-m>-0.2033</y-m>
				<z-m>-0.2380</z-m>
				<scale>1.0000</scale>
			</marker>
			<condition>
				<equals>
					<property>/controls/lighting/beacon</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/lighting/beacon</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>APU ECU</name>
			<value>PWR ON</value>
			<marker>
				<x-m>-15.4909</x-m>
				<y-m>0.0000</y-m>
				<z-m>-0.1893</z-m>
				<scale>1.0000</scale>
			</marker>
			<condition>
				<property>/controls/APU/electronic-control-unit</property>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/APU/electronic-control-unit</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>APU</name>
			<value>START</value>
			<marker>
				<x-m>-15.4909</x-m>
				<y-m>0.0314</y-m>
				<z-m>-0.1893</z-m>
				<scale>1.0000</scale>
			</marker>
			<condition>
				<property>/controls/APU/off-on</property>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/APU/off-on</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>APU/Generator</name>
			<value>ON</value>
			<marker>
				<x-m>-15.4215</x-m>
				<y-m>-0.1661</y-m>
				<z-m>-0.1554</z-m>
				<scale>1.4959</scale>
			</marker>
			<condition>
				<equals>
					<property>/controls/electric/APU-generator</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/electric/APU-generator</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>APU</name>
			<value>CHECK AVAIL</value>
			<marker>
				<x-m>-15.4926</x-m>
				<y-m>0.0132</y-m>
				<z-m>-0.1884</z-m>
				<scale>2.0248</scale>
			</marker>
			<condition>
				<and>
					<property>/controls/APU/off-on</property>
					<property>/engines/apu/running</property>
				</and>
			</condition>
		</item>

		<item>
			<name>Pushback</name>
			<value>AS REQUIRED</value>
		</item>
		<item>
			<name>Parking Brake (if pushback)</name>
			<value>RELEASE</value>
			<condition>
				<equals>
					<property>/controls/gear/brake-parking</property>
					<value>0</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/gear/brake-parking</property>
				<value>0</value>
			</binding>
		</item>
	</page>
</PropertyList>
