<?xml version="1.0" encoding="UTF-8" ?>

<!-- Marker positions for CRJ 700 / CRJ 900
	dx=1.96
</!-->

<PropertyList>
	<title>Pre-start</title>
	<page>
		<item>
			<name>Parking Brake</name>
			<value>CHECK SET</value>
			<condition>
				<equals>
					<property>/controls/gear/brake-parking</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/gear/brake-parking</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Landing gear lever</name>
			<value>CHECK DOWN</value>
			<marker>
				<x-m>-15.9190</x-m>
				<y-m>0.0339</y-m>
				<z-m>-1.0876</z-m>
				<scale>3.8760</scale>
			</marker>
			<condition>
				<equals>
					<property>/controls/gear/gear-down</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/gear/gear-down</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Throttle</name>
			<value>CUT OFF</value>
			<marker>
				<x-m>-15.5686</x-m>
				<y-m>-0.0240</y-m>
				<z-m>-1.1909</z-m>
				<scale>3.8760</scale>
			</marker>
			<condition>
				<and>
					<property>/controls/engines/engine[0]/cutoff</property>
					<property>/controls/engines/engine[1]/cutoff</property>
				</and>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/engines/engine[0]/cutoff</property>
				<value>1</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>/controls/engines/engine[1]/cutoff</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Flaps</name>
			<value>UP</value>
			<marker>
				<x-m>-15.6686</x-m>
				<y-m>0.1760</y-m>
				<z-m>-1.1909</z-m>
				<scale>3.8760</scale>
			</marker>
			<condition>
				<equals>
					<property>/surface-positions/flap-pos-norm</property>
					<value>0</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/flight/flaps</property>
				<value>0</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>controls.flapsDown(0);</script>
			</binding>
		</item>
		<item>
			<name>Battery Master</name>
			<value>ON</value>
			<view>
				<heading-offset-deg>321.1</heading-offset-deg>
				<pitch-offset-deg>28.2</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.5</x-offset-m>
				<y-offset-m>-0.5</y-offset-m>
				<z-offset-m>-15.0</z-offset-m>
				<field-of-view>55.0</field-of-view>
			</view>
			<marker>
				<x-m>-15.3488</x-m>
				<y-m>-0.1843</y-m>
				<z-m>-0.1273</z-m>
				<scale>1.4628</scale>
			</marker>
			<condition>
				<equals>
					<property>/controls/electric/battery-switch</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/electric/battery-switch</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Ground power</name>
			<value>SELECT</value>
			<view>
				<heading-offset-deg>321.1</heading-offset-deg>
				<pitch-offset-deg>28.2</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.5</x-offset-m>
				<y-offset-m>-0.5</y-offset-m>
				<z-offset-m>-15.0</z-offset-m>
				<field-of-view>55.0</field-of-view>
			</view>
			<marker>
				<x-m>-15.3397</x-m>
				<y-m>-0.1140</y-m>
				<z-m>-0.115</z-m>
				<scale>1.1488</scale>
			</marker>
			<condition>
				<property>/controls/electric/ac-service-selected</property>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/electric/ac-service-selected</property>
				<value>1</value>
			</binding>
		</item>

