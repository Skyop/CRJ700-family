<?xml version="1.0" encoding="UTF-8" ?>


<PropertyList>
	<title>Climb</title>
	<item>
		<name>Gear</name>
		<value>CHECK UP</value>
		<condition>
			<not>
				<property>controls/gear/gear-down</property>
			</not>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>controls/gear/gear-down</property>
			<value>0</value>
		</binding>
	</item>
	<item>
		<name>Flaps</name>
		<value>CHECK RECTRACTED</value>
		<condition>
			<not>
			<property>surface-positions/flap-pos-norm</property>
			</not>
			<not>
			<property>surface-positions/slat-pos-norm</property>
			</not>
		</condition>
	</item>
	<item>
		<name>Autopilot</name>
		<value>ON</value>
		<marker>
			<x-m>-15.7959</x-m>
			<y-m>-0.1405</y-m>
			<z-m>-0.6256</z-m>
			<scale>1.0000</scale>
		</marker>
		<condition>
			<property>/controls/autoflight/autopilot/engage</property>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/autoflight/autopilot/engage</property>
			<value>1</value>
		</binding>
	</item>
	<item>
		<name>Altimeter (above transition level)</name>
		<value>to NORM</value>
		<marker>
			<x-m>-15.9190</x-m>
			<y-m>-0.7215</y-m>
			<z-m>-1.0521</z-m>
			<scale>1.8760</scale>
		</marker>
	</item>
	<item>
		<name>Landing Light (ABOVE 10,000ft)</name>
		<value>OFF</value>
		<marker>
			<x-m>-15.6471</x-m>
			<y-m>-0.1545</y-m>
			<z-m>-0.3190</z-m>
			<scale>1.5124</scale>
		</marker>
		<condition>
				<and>
					<not><property>/controls/lighting/landing-lights[0]</property></not>
					<not><property>/controls/lighting/landing-lights[1]</property></not>
					<not><property>/controls/lighting/landing-lights[2]</property></not>
					<greater-equal>
						<property>/instrumentation/altimeter/indicated-altitude-ft</property>
						<value>10000</value>
					</greater-equal>
				</and>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/lighting/landing-lights[0]</property>
			<value>0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/lighting/landing-lights[1]</property>
			<value>0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/lighting/landing-lights[2]</property>
			<value>0</value>
		</binding>
	</item>
	<item>
		<name>FIXME</name>
		<value>...</value>
	</item>

</PropertyList>
